{
  "$schema": "https://claude-mcp-manager.dev/schema/registry.json",
  "version": "1.0.0",
  "mcps": {
    "supabase": {
      "name": "Supabase",
      "description": "Database operations, auth, storage, edge functions",
      "source": "npm:@supabase/mcp-server-supabase",
      "tags": ["database", "postgres", "auth", "storage"],
      "tokenCost": 5800,
      "config": {
        "command": "npx",
        "args": ["-y", "@supabase/mcp-server-supabase@latest"],
        "env": {
          "SUPABASE_ACCESS_TOKEN": "${SUPABASE_ACCESS_TOKEN}"
        }
      },
      "detection": {
        "packages": ["@supabase/supabase-js", "@supabase/ssr"],
        "envVars": ["SUPABASE_URL", "SUPABASE_ANON_KEY"],
        "files": ["supabase/config.toml"]
      }
    },
    "apify": {
      "name": "Apify",
      "description": "Web scraping, automation, and data extraction",
      "source": "npm:@apify/actors-mcp-server",
      "tags": ["scraping", "web", "automation", "data"],
      "tokenCost": 7200,
      "config": {
        "command": "npx",
        "args": ["-y", "@apify/actors-mcp-server"],
        "env": {
          "APIFY_TOKEN": "${APIFY_TOKEN}"
        }
      },
      "detection": {
        "packages": ["apify", "crawlee"],
        "envVars": ["APIFY_TOKEN"],
        "files": []
      }
    },
    "exa": {
      "name": "Exa",
      "description": "AI-powered web search and content retrieval",
      "source": "npm:exa-mcp-server",
      "tags": ["search", "web", "research"],
      "tokenCost": 650,
      "config": {
        "command": "npx",
        "args": ["-y", "exa-mcp-server"],
        "env": {
          "EXA_API_KEY": "${EXA_API_KEY}"
        }
      },
      "detection": {
        "packages": [],
        "envVars": ["EXA_API_KEY"],
        "files": []
      }
    },
    "pg": {
      "name": "PostgreSQL Docs",
      "description": "PostgreSQL documentation and query assistance",
      "source": "npm:@anthropic/pg-mcp-server",
      "tags": ["database", "postgres", "sql"],
      "tokenCost": 600,
      "config": {
        "command": "npx",
        "args": ["-y", "@anthropic/pg-mcp-server"]
      },
      "detection": {
        "packages": ["pg", "postgres", "@prisma/client"],
        "envVars": ["DATABASE_URL", "POSTGRES_URL"],
        "files": ["prisma/schema.prisma"]
      }
    },
    "github": {
      "name": "GitHub",
      "description": "GitHub repository operations, issues, PRs",
      "source": "npm:@anthropic/github-mcp-server",
      "tags": ["git", "github", "vcs"],
      "tokenCost": 1200,
      "config": {
        "command": "npx",
        "args": ["-y", "@anthropic/github-mcp-server"],
        "env": {
          "GITHUB_TOKEN": "${GITHUB_TOKEN}"
        }
      },
      "detection": {
        "packages": ["@octokit/rest", "@octokit/core"],
        "envVars": ["GITHUB_TOKEN"],
        "files": [".github/workflows"]
      }
    },
    "posthog": {
      "name": "PostHog",
      "description": "Product analytics, feature flags, experiments",
      "source": "npm:@posthog/mcp-server",
      "tags": ["analytics", "tracking", "experiments"],
      "tokenCost": 4500,
      "config": {
        "command": "npx",
        "args": ["-y", "@posthog/mcp-server"],
        "env": {
          "POSTHOG_API_KEY": "${POSTHOG_API_KEY}"
        }
      },
      "detection": {
        "packages": ["posthog-js", "posthog-node"],
        "envVars": ["POSTHOG_API_KEY", "NEXT_PUBLIC_POSTHOG_KEY"],
        "files": []
      }
    },
    "fetch": {
      "name": "Web Fetch",
      "description": "Fetch and parse web pages",
      "source": "npm:@anthropic/fetch-mcp-server",
      "tags": ["web", "fetch", "http"],
      "tokenCost": 400,
      "config": {
        "command": "npx",
        "args": ["-y", "@anthropic/fetch-mcp-server"]
      },
      "detection": {
        "packages": [],
        "envVars": [],
        "files": []
      }
    },
    "filesystem": {
      "name": "Filesystem",
      "description": "Extended file system operations",
      "source": "npm:@anthropic/filesystem-mcp-server",
      "tags": ["files", "filesystem"],
      "tokenCost": 350,
      "config": {
        "command": "npx",
        "args": ["-y", "@anthropic/filesystem-mcp-server"]
      },
      "detection": {
        "packages": [],
        "envVars": [],
        "files": []
      }
    }
  }
}
